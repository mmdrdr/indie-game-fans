<div class="right-all col-8 offset-1">
  <% if @model == "game" %>
    <h1>キーワード"<%= @content %>"でけんさくしたゲームたち</h1>
    <%= render 'games/index', games: @records %>
  <% else @model == "user" %>
    <h1>キーワード"<%= @content %>"でけんさくしたクリエイターたち</h1>
    <%= render 'users/index', users: @records %>
  <% end %>
</div>